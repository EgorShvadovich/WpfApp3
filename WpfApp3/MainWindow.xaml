<Window x:Class="WpfApp3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Canvas>
            <Ellipse Canvas.Left="100" Canvas.Top="50"
               Width="200" Height="120" StrokeThickness="9" Stroke="Gray">
            </Ellipse>
            <Ellipse Canvas.Left="135" Canvas.Top="50"
               Width="130" Height="55" StrokeThickness="9" Stroke="Gray">
            </Ellipse>
            <Ellipse Canvas.Left="227.5" StrokeThickness="9" Stroke="Gray" Canvas.Top="50" 
               Width="120" Height="55">
                <Ellipse.RenderTransform>
                    <RotateTransform Angle="90" />
                </Ellipse.RenderTransform>
            </Ellipse>

        </Canvas>

    </Grid>
</Window>
